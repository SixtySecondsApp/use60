# =============================================================================
# use60 - Environment Configuration Template
# =============================================================================
# Copy this file to .env and fill in your values
# See also: .env.production, .env.staging for environment-specific configs
# =============================================================================

# -----------------------------------------------------------------------------
# SUPABASE - Primary Database (Required)
# -----------------------------------------------------------------------------
# Get these from: Supabase Dashboard > Settings > API
# Note: Both VITE_ prefixed (client-side) and non-prefixed (Vercel build) versions needed
# Client-side variables (VITE_ prefix exposes to browser via import.meta.env)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
# Server-side/build variables (non-prefixed for Vercel compatibility)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
# Service credentials (never exposed to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ACCESS_TOKEN=your-access-token
SUPABASE_PROJECT_ID=your-project-id

# -----------------------------------------------------------------------------
# ENVIRONMENT
# -----------------------------------------------------------------------------
# Controls Sentry initialization - only enabled in production
# Options: development | staging | production
VITE_ENVIRONMENT=development

# -----------------------------------------------------------------------------
# APPLICATION URLS
# -----------------------------------------------------------------------------
# Local: http://localhost:5175 | Prod: https://app.use60.com
VITE_PUBLIC_URL=http://localhost:5175
PUBLIC_URL=http://localhost:5175
APP_URL=http://localhost:5175
SITE_URL=http://localhost:5175

# -----------------------------------------------------------------------------
# AUTHENTICATION - Clerk (Optional)
# -----------------------------------------------------------------------------
# Get these from: Clerk Dashboard > API Keys
VITE_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=
VITE_USE_CLERK_AUTH=false
VITE_MULTI_TENANT_ENABLED=true

# -----------------------------------------------------------------------------
# AWS SES - Email (Required for email features)
# -----------------------------------------------------------------------------
AWS_REGION=eu-west-2
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
SES_FROM_EMAIL=app@yourdomain.com
SES_FROM_NAME=Your App Name
AWS_S3_BUCKET=your-bucket-name

# -----------------------------------------------------------------------------
# AI PROVIDERS (Required for AI features)
# -----------------------------------------------------------------------------
VITE_OPENAI_API_KEY=
VITE_ANTHROPIC_API_KEY=
VITE_OPENROUTER_API_KEY=
VITE_GEMINI_API_KEY=

# -----------------------------------------------------------------------------
# INTEGRATIONS - Slack
# -----------------------------------------------------------------------------
SLACK_CLIENT_ID=
SLACK_CLIENT_SECRET=
SLACK_SIGNING_SECRET=
VITE_SLACK_CLIENT_ID=

# -----------------------------------------------------------------------------
# INTEGRATIONS - Google
# -----------------------------------------------------------------------------
# Get these from: Google Cloud Console > APIs & Services > Credentials
VITE_GOOGLE_CLIENT_ID=
VITE_GOOGLE_CLIENT_SECRET=
VITE_GOOGLE_REDIRECT_URI=http://localhost:5175/auth/google/callback

# -----------------------------------------------------------------------------
# INTEGRATIONS - Fathom (Meeting Transcripts)
# -----------------------------------------------------------------------------
VITE_FATHOM_CLIENT_ID=
VITE_FATHOM_CLIENT_SECRET=
VITE_FATHOM_REDIRECT_URI=
FATHOM_WEBHOOK_PROXY_SECRET=

# -----------------------------------------------------------------------------
# INTEGRATIONS - HubSpot
# -----------------------------------------------------------------------------
HUBSPOT_CLIENT_ID=
HUBSPOT_CLIENT_SECRET=
VITE_HUBSPOT_INTEGRATION_ENABLED=false

# -----------------------------------------------------------------------------
# INTEGRATIONS - Stripe (Payments)
# -----------------------------------------------------------------------------
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# -----------------------------------------------------------------------------
# INTEGRATIONS - Other Services
# -----------------------------------------------------------------------------
SAVVYCAL_API_TOKEN=
VITE_FREEPIK_API_KEY=
GLADIA_API_KEY=
DEEPGRAM_API_KEY=
ASSEMBLYAI_API_KEY=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

# -----------------------------------------------------------------------------
# INTEGRATIONS - MeetingBaaS (Meeting Recording)
# -----------------------------------------------------------------------------
MEETINGBAAS_API_KEY=
MEETINGBAAS_WEBHOOK_SECRET=

# -----------------------------------------------------------------------------
# MONITORING
# -----------------------------------------------------------------------------
VITE_SENTRY_DSN=
VITE_SENTRY_ENABLED=false
SENTRY_WEBHOOK_SECRET=

# -----------------------------------------------------------------------------
# SECURITY
# -----------------------------------------------------------------------------
CRON_SECRET=

# -----------------------------------------------------------------------------
# TESTING
# -----------------------------------------------------------------------------
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=TestPassword123!
